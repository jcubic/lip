{
    "name": "jcubic/lip",
    "description": "LIP is LIPS In Php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub T. Jankiewicz",
            "email": "jcubic@onet.pl"
        }
    ],
    "config": {
        "platform": {
            "php": "7.2.0"
        }
    },
    "autoload": {
        "files": ["src/utils.php", "src/tokenizer.php"],
        "psr-4": {
            "jcubic\\lip\\": "src/"
        }
    },
    "autoload-dev": {
        "files": ["src/utils.php", "src/tokenizer.php"],
        "psr-4": {
            "jcubic\\lips\\": "tests/"
        }
    },
    "require": {
        "phpmath/biginteger": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "php-coveralls/php-coveralls": "^2.1"
    }
}
